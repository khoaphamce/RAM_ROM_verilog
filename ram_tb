    %flag_set/imm 4, 0;
    %store/vec4a v0x559d2dc3c750, 4, 0;
    %pushi/vec4 5, 0, 4;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x559d2dc3c750, 4, 0;
T_0.0 ;
    %load/vec4 v0x559d2dc3c8d0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x559d2dc3c480_0;
    %load/vec4 v0x559d2dc13de0_0;
    %pad/u 4;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x559d2dc3c750, 0, 4;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x559d2dc3c8d0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x559d2dc13de0_0;
    %pad/u 4;
    %ix/vec4 4;
    %load/vec4a v0x559d2dc3c750, 4;
    %store/vec4 v0x559d2dc3c620_0, 0, 4;
T_0.4 ;
T_0.3 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x559d2dbee130;
T_1 ;
    %vpi_call 2 8 "$dumpfile", "ram_tb.vcd" {0 0 0};
