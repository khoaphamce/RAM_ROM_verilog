    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x559d2dc28730;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %load/vec4 v0x559d2dc3ca50_0;
    %addi 1, 0, 3;
    %store/vec4 v0x559d2dc3ca50_0, 0, 3;
    %jmp T_3.0;
T_3.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559d2dc3cd40_0, 0, 1;
    %pushi/vec4 0, 0, 3;
