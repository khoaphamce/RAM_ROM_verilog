    .scope S_0x559d2dbee130;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x559d2dc3cb30_0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559d2dc3cd40_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x559d2dc3ca50_0, 0, 3;
T_3.0 ;
    %load/vec4 v0x559d2dc3ca50_0;
    %pad/u 32;
    %cmpi/u 3, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_3.1, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559d2dc3ce30_0, 0, 1;
    %pushi/vec4 2, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
